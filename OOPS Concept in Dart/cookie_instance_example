void main() {
  // Creating an instance of the Cookie class
  Cookie cookie = Cookie();
  
  // Accessing and printing the instance variables
  print(cookie.quantity); // Output: 2 (default value of quantity)
  print(cookie.shape);    // Output: Star (default value of shape)
  
  // Calling methods of the Cookie instance
  cookie.baking();        // Initiates the baking process
  print(cookie.isBaked()); // Checks and prints if the cookie is baked (false)
}

class Cookie {
  // Instance Variables -> Represent the data stored in each instance
  String shape = "Star"; // Default shape of the cookie
  int quantity = 2;      // Default quantity of cookies
  
  // Method -> Represents the actions or behaviors of the class
  void baking() {
    // Prints a message indicating the baking process has started
    print("The baking process has been initiated.");
  }
  
  bool isBaked() {
    // Returns the baked status of the cookie (default: false)
    return false;
  }
}


Output:
2
Star
The baking process has been initiated.
false

